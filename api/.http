###
# Upload contract ABI
POST http://localhost:3004/contracts/upload
Content-Type: application/json

{
  "name": "MyNFTContract",
  "abi": {
    "name": "nft_contract",
    "outputs": {
      "structs": {
        "events": [
          {
            "kind": "struct",
            "path": "nft_contract::Transfer",
            "fields": [
              { "name": "from", "type": { "kind": "struct", "path": "aztec::protocol_types::address::aztec_address::AztecAddress", "fields": [{ "name": "inner", "type": { "kind": "field" } }] } },
              { "name": "to", "type": { "kind": "struct", "path": "aztec::protocol_types::address::aztec_address::AztecAddress", "fields": [{ "name": "inner", "type": { "kind": "field" } }] } },
              { "name": "token_id", "type": { "kind": "field" } }
            ]
          },
          {
            "kind": "struct",
            "path": "nft_contract::Mint",
            "fields": [
              { "name": "to", "type": { "kind": "struct", "path": "aztec::protocol_types::address::aztec_address::AztecAddress", "fields": [{ "name": "inner", "type": { "kind": "field" } }] } },
              { "name": "token_id", "type": { "kind": "field" } }
            ]
          }
        ]
      }
    }
  }
}

