{
  "type": "module",
  "name": "continuum-api",
  "version": "1.0.0",
  "private": true,
  "main": "app.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "node --test test/**/*.test.js",
    "start": "fastify start -w -l info -P -p 3004 -a 0.0.0.0 --body-limit 104857600 app.js",
    "dev": "fastify start -w -l info -P -p 3004 -a 0.0.0.0 --body-limit 104857600 app.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "delete:collections": "bun run scripts/delete-collections.js",
    "delete:collections:dry-run": "bun run scripts/delete-collections.js --dry-run",
    "delete:collections:force": "bun run scripts/delete-collections.js --force"
  },
  "dependencies": {
    "@aztec/bb.js": "1.1.2",
    "@clerk/fastify": "^1.0.35",
    "@fastify/autoload": "^5.9.0",
    "@fastify/cors": "^9.0.1",
    "@fastify/env": "^4.3.0",
    "@fastify/mongodb": "^8.0.0",
    "@fastify/multipart": "6",
    "@fastify/sensible": "^5.6.0",
    "@fastify/swagger": "^8.14.0",
    "@fastify/swagger-ui": "^4.0.0",
    "@spiceai/spice": "^2.0.0",
    "@supabase/supabase-js": "^2.50.2",
    "@zkpassport/sdk": "^0.12.4",
    "adm-zip": "^0.5.16",
    "bull": "^4.16.5",
    "canvas": "^3.2.0",
    "ethers": "^6.13.0",
    "fastify": "^4.28.0",
    "fastify-cli": "^6.2.1",
    "fastify-plugin": "^4.0.0",
    "fastify-raw-body": "^4.0.0",
    "node-stream-zip": "^1.15.0",
    "uuid": "^11.1.0",
    "zod": "^3.25.72"
  },
  "devDependencies": {
    "c8": "^10.1.2",
    "eslint": "^9.5.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsdoc": "^48.2.12",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.3.2"
  },
  "eslintIgnore": [
    "dist/"
  ]
}
